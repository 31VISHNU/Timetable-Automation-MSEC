@model TTGenerator.Models.View_tt

<html>
<style>
    .done {
        background-color: #ADD8E6;
        color: white;
    }
</style>
<body>
    @using (Html.BeginForm("View_tt","Home",FormMethod.Post))
    {
    <div align="center" class="done">
        <p>
            MEPCO SCHLENK ENGINEERING COLLEGE, SIVAKASI
        </p>
        <p>(An Autonomous Institution - Affiliated to Anna University, Chennai)</p>
        <p>UG Programmes - NBA Accredited (TIER - I), NAAC Accredited with 'A' Grade</p>
        <p>Email:msec@mepcoeng.ac.in Phone:+91-4562-23500(30 Lines) Fax:+91-4562-235111</p>
    </div>
    <label>
        <h1>
            Year
            <select id="year" asp-for="year_s">
                <option value="I">I</option>
                <option value="II">II</option>
                <option value="III">III</option>
                <option value="IV">IV</option>
            </select>
        </h1>
        <table id="table1">
        </table>
    </label>
    <label>
        <h1>
            Semester_id
            <select id="semester" asp-for="semester_id">
                <option value=1>1</option>
                <option value=2>2</option>
                <option value=3>3</option>
                <option value=4>4</option>
                <option value=5>5</option>
                <option value=6>6</option>
                <option value=7>7</option>
                <option value=8>8</option>
            </select>
        </h1>
    </label>
    <label>
        <h1>
            Batch_start_year
            <input type="number" id="batch_start_year" asp-for="batch_start_year" />
        </h1>
    </label>
    <input type="submit" />
    }
</body>
</html>