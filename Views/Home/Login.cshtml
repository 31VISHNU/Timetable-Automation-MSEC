@model TTGenerator.Models.Login
<html>
<head>
    <style>
        .su {
            background-color: #ADD8E6;
            color: white;
            padding: 14px 20px;
            margin: 8px 0;
            border: none;
            cursor: pointer;
        }

    .su:hover {
    opacity: 0.8;
    }
    .container {
        padding: 16px;
     }
    .done {
        background-color: #ADD8E6;
        color: white;
    }
        .card {
            box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
            transition: 0.3s;
            width: 30%;
            border-radius: 5px;
            background-color: #ADD8E6;
        }
        .card2 {
            box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
            transition: 0.3s;
            width: 100%;
            border-radius: 5px;
            background-color: #ADD8E6;
        }

            .card:hover {
                box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2);
            }
    </style>
</head>
<body>
    <div align="center" class="done">
        <p>
            MEPCO SCHLENK ENGINEERING COLLEGE, SIVAKASI
        </p>
        <p>(An Autonomous Institution - Affiliated to Anna University, Chennai)</p>
        <p>UG Programmes - NBA Accredited (TIER - I), NAAC Accredited with 'A' Grade</p>
        <p>Email:msec@mepcoeng.ac.in Phone:+91-4562-23500(30 Lines) Fax:+91-4562-235111</p>
    </div>
    <div class="container" align="center">
        <h1>MSEC TIME TABLE AUTOMATION</h1>
        <form method="post" action="Home/Login">
            <label>
                Login:
                <input type="text" asp-for="login_id" />
            </label>
            <br />
            <br />
            <label>
                Password:
                <input type="password" asp-for="password_l" />
            </label>
            <br />
            <br />
            <input class="su" type="submit" />
        </form>
    </div>
    <div class="card2" align="center">
        <p>MSEC                                                                       developed by AIDS</p>
    </div>
</body>
</html>